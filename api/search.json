---
[
  {% for post in site.posts %}
    { "label": "{{ post.title }}", "value": "{{ post.url | relative_url }}"},
  {% endfor %}
  {% for person in site.people %}
    { "label": "{{ person.name }}", "value": "{{ person.url | relative_url }}"},
  {% endfor %}
  {% for page in site.program %}
    { "label": "{{ page.title }}", "value": "{{ page.url | relative_url }}"},
  {% endfor %}
  {% for page in site.pages %}
    { "label": "{{ page.title }}", "value": "{{ page.url | relative_url }}"}{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
